{
    "name": "deepcode-tech/laravel-multi-language",
    "description": "适用于laravel的多语言支持扩展",
    "type": "library",
    "keywords": ["laravel", "extension"],
    "homepage": "https://github.com/deepcode-tech/laravel-multi-language",
    "license": "MIT",
    "version": "0.0.1",
    "authors": [
        {
            "name": "stanzhou",
            "email": "m@sq.mk"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "laravel/framework": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Deepcode\\MultiLanguage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Deepcode\\MultiLanguage\\MultiLanguageServiceProvider"
            ]
        }
    }
}
